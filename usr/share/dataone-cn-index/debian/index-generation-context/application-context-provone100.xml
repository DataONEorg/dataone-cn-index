<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="provRdfXmlSubprocessor" class="org.dataone.cn.indexer.annotation.RdfXmlSubprocessor">
		<property name="matchDocuments">
			<list>
				<value>http://www.w3.org/TR/rdf-syntax-grammar</value>
				<value>http://www.openarchives.org/ore/terms</value>
			</list>
		</property>	
		<property name="fieldList">
			<list>
                <ref bean="prov.wasDerivedFrom" />
                <ref bean="prov.wasGeneratedBy" />
                <ref bean="prov.wasInformedBy" />
                <ref bean="prov.used" />
                <ref bean="prov.generatedByProgram" />
                <ref bean="prov.generatedByExecution" />
                <ref bean="prov.generatedByUser" />
                <ref bean="prov.usedByProgram" />
                <ref bean="prov.usedByExecution" />
                <ref bean="prov.usedByUser" />
                <ref bean="prov.wasExecutedByExecution" />
                <ref bean="prov.wasExecutedByUser" />
                <ref bean="prov.instanceOfClass" />
                <ref bean="prov.hasSources" />
                <ref bean="prov.hasDerivations" />
			</list>
		</property>	
	</bean>
</beans>